
@{
    ViewData["Title"] = "Cancel";
    Layout = "_LayoutAdmin";
}
 
<section id="donate" class="pad-top donate-wrapper bg-gray-1">
                    <div class="container">
                        <h1 data-aos="fade-up" data-aos-delay="200" class="text-capitalize text-center font3">
                            Cancel Membership
                        </h1>
                        We're sorry that you want to cancel your membership.  Please click on button below to cancel.

                        <form asp-action="Cancel" class="contact-form form-dark mt-5 ">
                            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                            <button type="submit" class="btn btn-block btn-lg btn-dark">Cancel Membership</button>
                        </form>
                    </div>
                </section>
